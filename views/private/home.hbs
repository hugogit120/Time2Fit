{{>navbar}}

<div class="title-page homedeborrar">

    <h1 class="title-page">Exercises</h1>
</div>


<section class="exercises-container">
    {{#each exercise}}
    <div class="exercise-card">
        <a class="exercise-title" href="exercise/{{this._id}}">{{this.title}}</a>
        <a href="exercise/{{this._id}}"><img
                src="https://fitnessexpertawards.com/wp-content/uploads/2019/06/female-squat-form.jpg" alt=""></a>
        <form action="/private/add/favorite/{{this._id}}" method="POST">
            <button type="submit" class="edit-button">ADD TO FAVORITE </button>
        </form>
    </div>
    {{/each}}
</section>

{{>footer}}